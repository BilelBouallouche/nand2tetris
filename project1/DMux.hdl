/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
      And(a=sel, b=true, out=sel_cond);
      Not(in=sel_cond, out=not_selcond);
      And(a=in, b=not_selcond, out=a);
      And(a=in, b=sel_cond, out=b);
}